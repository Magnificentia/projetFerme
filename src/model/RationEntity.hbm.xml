<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.RationEntity" table="ration" schema="volailledor">
        <id name="idRation">
            <column name="idRation" sql-type="int(10) unsigned"/>
        </id>
        <property name="dateRation">
            <column name="dateRation" sql-type="datetime"/>
        </property>
        <property name="qte">
            <column name="qte" sql-type="decimal(5,2)" precision="5" scale="2"/>
        </property>
        <property name="eau">
            <column name="eau" sql-type="decimal(5,2)" precision="5" scale="2"/>
        </property>
    </class>
</hibernate-mapping>