<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.StockalimentEntity" table="stockaliment" schema="volailledor">
        <id name="idStock">
            <column name="idStock" sql-type="int(10) unsigned"/>
        </id>
        <property name="qte">
            <column name="qte" sql-type="int(10) unsigned"/>
        </property>
        <property name="dateArrivage">
            <column name="dateArrivage" sql-type="datetime" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>