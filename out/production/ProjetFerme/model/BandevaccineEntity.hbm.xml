<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.BandevaccineEntity" table="bandevaccine" schema="volailledor">
        <id name="idBandeVaccine">
            <column name="idBandeVaccine" sql-type="int(10) unsigned"/>
        </id>
        <property name="dateVac">
            <column name="dateVac" sql-type="datetime" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>